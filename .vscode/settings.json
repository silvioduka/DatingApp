{
    "files.associations": {
        "*.yaml": "home-assistant"
    },
    "maven.view": "hierarchical",
    "errorLens.gutterIconsEnabled": true,
    "errorLens.fontWeight": "600",
    "errorLens.fontStyleItalic": true,
    "errorLens.gutterIconSet": "defaultOutline",
    "errorLens.addAnnotationTextPrefixes": true,
    "errorLens.exclude": [
        "expected call-signature"
    ],
    "errorLens.followCursorMore": 1,
    "errorLens.margin": "5ch"
}